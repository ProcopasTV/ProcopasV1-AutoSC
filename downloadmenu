#!/bin/bash
# ==========================================
# Color
# download menu
cd /usr/bin
wget -O menu "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/menu"
wget -O menu-trojan "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/menu-trojan"
wget -O menu-vmess "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/menu-vmess"
wget -O cek-service.sh "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/cek-service.sh"
wget -O clear-log "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/clear-log.sh"
wget -O delexp "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/delexp.sh"
wget -O xp "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/xp.sh"
wget -O cert "https://github.com/ProcopasTV/ProcopasV1-AutoSC/blob/main/cert.sh"
wget -O add-vmess "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/add-vmess"
wget -O del-vmess "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/del-vmess"
wget -O renew-vmess "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/renew-vmess"
wget -O cek-vmess.sh "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/cek-vmess.sh"
wget -O add-trojan "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/add-trojan"
wget -O del-trojan "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/del-trojan"
wget -O renew-trojan "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/renew-trojan"
wget -O cek-trojan.sh "https://raw.githubusercontent.com/ProcopasTV/ProcopasV1-AutoSC/main/cek-trojan.sh"
wget -O sl-download-info "https://github.com/ProcopasTV/ProcopasV1-AutoSC/blob/main/speedtest"
chmod +x /usr/bin/menu
chmod +x /usr/bin/menu-vmess
chmod +x /usr/bin/menu-trojan
chmod +x /usr/bin/cert.sh
chmod +x /usr/bin/clear-log.sh
chmod +x /usr/bin/delexp.sh
chmod +x /usr/bin/xp.sh
chmod +x /usr/bin/cek-service.sh
chmod +x /usr/bin/speedtest
chmod +x /usr/bin/downloadmenu
chmod +x /usr/bin/add-vmess
chmod +x /usr/bin/cek-vmess.sh
chmod +x /usr/bin/renew-vmess
chmod +x /usr/bin/del-vmess
chmod +x /usr/bin/add-trojan
chmod +x /usr/bin/del-trojan
chmod +x /usr/bin/renew-trojan
chmod +x /usr/bin/cek-trojan.sh
chmod +x /usr/bin/downloadmenu
cd
